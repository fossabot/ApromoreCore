<!-- editprocessdata window has EditDataOneProcess.java as a controller
for editing purpose 
and CreateProcessController.java for creating process purpose.
-->

<window width="400px" closable="false" position="center" id="winFolderCreate">
    <vbox vflex="1">
        <grid sclass="ap-grid">
            <columns sizable="true">
                <column width="30%"></column>
                <column width="70%"></column>
            </columns>
            <rows>
                <row tooltiptext="Enter folder name.">
                    <label value="Folder name" width="100%"/>
                    <textbox id="txtName" width="100%"/>
                </row>
<!--
                <row tooltiptext="GED Matrix Computation.">
                    <label value="GED Matrix Required" width="100%"/>
                    <checkbox id="checkboxGED" width="100%"/>
                </row>
-->
                <row spans="3">
                    <div align="center">
                        <button id="btnSave" label="Save" iconSclass="z-icon-check-circle"></button>
                        <button id="btnCancel" label="Cancel" iconSclass="z-icon-times-circle"></button>
                    </div>
                </row>
            </rows>
        </grid>
    </vbox>
</window>
